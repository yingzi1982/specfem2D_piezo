subroutine finalize_piezo()

use constants, only: IMAIN
use piezo_par, only: voltage_time_function, charges

implicit none

write(IMAIN,*) 'piezo', voltage_time_function(1)
deallocate(voltage_time_function)
deallocate(charges)

end subroutine
